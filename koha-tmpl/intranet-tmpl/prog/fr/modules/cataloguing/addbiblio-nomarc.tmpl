<!-- TMPL_INCLUDE NAME="doc-head-open.inc" -->
<title>Koha &rsaquo; Catalogage &rsaquo; ajouter une notice non-MARC</title>
<!-- TMPL_INCLUDE NAME="doc-head-close-addbiblio.inc" -->



<!-- TMPL_IF name="ERROR" -->
Précisez un titre SVP
<!-- /TMPL_IF -->

<form action="savebiblio.pl" method="post">

<table>
	<caption>Ajouter une nouvelle notice bibliographique</caption>
	<tr><th scope="row"><label for="title">Titre : *</label></th><td><input name="title" id="title" type="text" size="40" /> * Obligatoire</td>
</tr>
<tr><th scope="row"><label for="subtitle">Sous-titre : </label></th><td><input name="subtitle" id="subtitle" type="text" size="40" /></td>
</tr>
<tr><th scope="row"><label for="author">Auteur : </label></th><td><input name="author" id="author" type="text" size="40" /></td>
</tr>
<tr><th scope="row"><label for="seriestitle">Titre de collection : </label><br>
<i>(si applicable)</i></th><td><input name="seriestitle" type="text" id="seriestitle" size="40" /></td>
</tr>
<tr><th scope="row"><label for="copyrightdate">Date de copyright :</label></th><td><input id="copyrightdate" name="copyrightdate" type="text" size="40" /></td>
</tr>
<tr><th scope="row"><label for="abstract">Résumé : </label></th><td><textarea cols="30" id="abstract" rows="6" name="abstract"></textarea></td>
</tr>
<tr><th scope="row"><label for="bib-notes">Notes : </label></th><td><textarea cols="30" rows="6" id="bib-notes" name="notes"></textarea></td>
</tr>
</table>
<input type="submit" value="Ajouter une notice">

</form>
