                                                            >[% basketgroup.name | html %] <span>(closed)</span></a
                                                        >
                                                    [% ELSE %]
                                                        [% basketgroup.name | html %] <span>(closed)</span>
                                                    [% END %]
                                                [% ELSIF ( ! CAN_user_acquisition_group_manage ) %]
