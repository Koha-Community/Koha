                                                                        <option value="[% bg.id | html %]" selected="selected">[% bg.name | html %]</option>
                                                                    [% ELSE %]
                                                                        [% UNLESS bg.closed %]
                                                                            <option value="[% bg.id | html %]">[% bg.name | html %]</option>
                                                                        [% ELSE %]
                                                                            <option value="[% bg.id | html %]" disabled="disabled">[% bg.name | html %] <span>(closed)</span></option>
                                                                        [% END %]
                                                                    [% END %]
                                                                [% END %]
