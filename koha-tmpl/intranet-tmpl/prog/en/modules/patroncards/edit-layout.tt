                                                    <option value="[% field_2_fon.type | html %]" selected="selected">[% field_2_fon.name | html %]</option>
                                                    [% ELSE %]
                                                    <option value="[% field_2_fon.type | html %]">[% field_2_fon.name | html %]</option>
                                                    [% END %]
                                                    [% END %]
                                                </select>
                                            </li>
                                            <li>
