        <select name="printer" id="printer">
            <!-- TMPL_LOOP Name="printerloop" -->
                <!-- TMPL_IF NAME="selected" -->
                    <option value="<!-- TMPL_VAR NAME="value" -->" selected="selected"><!-- TMPL_VAR NAME="name" --></option>
