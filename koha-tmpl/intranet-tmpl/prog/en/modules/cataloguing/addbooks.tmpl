        <!-- TMPL_IF name="even" -->
            <tr class="highlight">
        <!-- TMPL_ELSE -->
            <tr>
        <!-- /TMPL_IF -->
            <td>
                <p>
                    <a href="/cgi-bin/koha/catalogue/MARCdetail.pl?biblionumber=<!-- TMPL_VAR name="biblionumber" -->"><!-- TMPL_VAR NAME="title" --></a>
                <!-- TMPL_VAR NAME="subtitle" --></p>
                <!-- TMPL_IF name="summary" -->
                    <p><!-- TMPL_VAR name="summary" --></p>
                <!-- TMPL_ELSE -->
                    <p>
                        <!-- TMPL_IF NAME="author" -->
