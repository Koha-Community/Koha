                                                        [% ELSE %]
                                                            <span class="unavailable">No items</span>
                                                        [% END %]
                                                    [% END # /IF ( SEARCH_RESULT.items_count ) %]
                                                </div> <!-- /.availability -->
                                            </td>
                                        </tr>
                                    [% END # /FOREACH SEARCH_RESULT %]
                                </table>
                            </form>
                        </div> <!-- /#bookbag_form -->
                    [% END # /IF scan %]
                    [% INCLUDE 'page-numbers.inc' %]
                [% ELSE #/IF total %]
