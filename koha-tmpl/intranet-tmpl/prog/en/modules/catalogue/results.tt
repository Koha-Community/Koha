    <!-- Value will be set here by addToList() -->
    <input id="list_form_biblios" type="hidden" name="biblionumbers" value="" />
    <input type="hidden" name="multi_listadd" value="1"/>
</form>

</div>
</div>
<div class="yui-b">
[% INCLUDE 'facets.inc' %]
</div>
