    },
    "enumchron": {
      "type": ["string", "null"],
      "description": "serial enumeration/chronology for the item"
    },
    "copynumber": {
      "type": ["string", "null"],
      "description": "copy number"
    },
    "stocknumber": {
      "type": ["string", "null"],
      "description": "inventory number"
    },
    "new_status": {
      "type": ["string", "null"],
      "description": "'new' value, whatever free-text information."
    },
    "datereceived": {
      "type": ["string", "null"],
      "description": "When this Item was received.",
      "format": "date-time"
    },
    "genre": {
      "type": ["string", "null"],
      "description": "GENRE (MARC21 952$G)"
    },
    "sub_location": {
      "type": ["string", "null"],
      "description": "SUBLOC (MARC21 952$S)"
    },
    "circulation_level": {
      "type": ["string", "null"],
      "description": "circulation level authorized value"
    },
    "reserve_level": {
      "type": ["string", "null"],
      "description": "reserve level authorized value"
    },
    "holding_id": {
      "type": ["string", "null"],
      "description": "internally assigned holding identifier"
    }
